<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android" >    
    <item android:state_selected="true">
    	<shape >
    	    <solid android:color="@color/color_white"/>
    	    <corners android:radius="@dimen/eq_content_indicator_radius"/>
    	    <size android:width="@dimen/eq_content_indicator_select_length"
    	        android:height="@dimen/eq_content_indicator_normal_length"/>
    	</shape>
    </item>
    
	<item android:state_selected="false">
	    <shape >
    	    <solid android:color="@color/color_white"/>
    	    <corners android:radius="@dimen/eq_content_indicator_radius"/>
    	    <size android:width="@dimen/eq_content_indicator_normal_length"
    	        android:height="@dimen/eq_content_indicator_normal_length"/>
    	</shape>
	</item>
</selector>